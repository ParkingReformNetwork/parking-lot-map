{
  "name": "parking-lot-map",
  "version": "1.0.0",
  "source": [
    "index.html",
    "data/cities-polygons.geojson",
    "data/parking-lots.geojson"
  ],
  "scripts": {
    "start": "parcel",
    "build": "rm -rf dist; parcel build",
    "test": "jest",
    "fmt": "prettier --write .",
    "fix": "prettier --write scripts/ src/js/ src/tests; eslint --fix scripts/ src/",
    "lint": "prettier --check . && eslint scripts/ src/",
    "update-city-boundaries": "node scripts/update-city.js",
    "update-lots": "node scripts/update-lots.js"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "puppeteer": "^19.8.2"
  },
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
